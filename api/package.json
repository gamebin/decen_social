{
  "name": "decen_social-api",
  "description": "Decentralized social service REST APIs using express, mysql in ES6 with code coverage",
  "version": "1.0.0",
  "author": "GAMEKIKI",
  "main": "index.js",
  "engines": {
    "node": ">=8.15.0",
    "npm": ">=6.4.0",
    "yarn": ">=1.15.2"
  },
  "scripts": {
    "start": "node index.js",
    "start:debug": "cross-env DEBUG=decen_social-api:* yarn start",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gamebin/decen_social"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "console-stamp": "^0.2.9",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-validation": "1.0.2",
    "express-winston": "2.5.0",
    "helmet": "^3.21.2",
    "http-status": "1.0.1",
    "joi": "10.6.0",
    "jsonwebtoken": "7.1.9",
    "method-override": "^2.3.10",
    "mongoose": ">=5.7.5",
    "morgan": ">=1.9.1",
    "mysql2": "^1.6.5",
    "prettier": ">=1.13.0",
    "sequelize": "^5.21.2",
    "winston": "2.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "braces": "^3.0.2",
    "chai": "4.1.2",
    "commitizen": "^3.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "cz-conventional-changelog": "1.2.0",
    "debug": "^2.6.9",
    "diff": "^4.0.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-watch": "^6.0.0",
    "faker": "^4.1.0",
    "growl": "^1.10.5",
    "hoek": "^6.1.3",
    "husky": "0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "jsdiff": "^1.1.1",
    "lodash": "^4.17.15",
    "merge": "^1.2.1",
    "mocha": "^6.0.0",
    "node-growl": "^0.1.1",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0",
    "ws": "^7.4.6"
  },
  "license": "GPL-2.0-or-later",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
